    "headers['Content-Type']='application/json'\n",
    "\n",
    "apiRequest = f\"{APIURL}/v1/api/pipelines/create\"\n",
    "\n",
    "pipeline_name=f\"{suffix}apiinferenceexamplepipeline\"\n",
